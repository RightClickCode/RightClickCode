{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus build",
        "outputPath": "dist/apps/docs"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus serve"
      }
    },
    "swizzle": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus swizzle"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus deploy"
      }
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus clear"
      }
    },
    "write-translations": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus write-translations"
      }
    },
    "write-heading-ids": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "docusaurus write-heading-ids"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/docs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/docs/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "typescript-check": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs",
        "command": "tsc -p tsconfig.json"
      }
    }
  }
}
